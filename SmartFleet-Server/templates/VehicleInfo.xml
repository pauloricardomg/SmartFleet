$def with (vehicles)
<?xml version="1.0"?>
<VehicleInfo>
	$for entry in vehicles:
	<Vehicle>
		<id>$entry.id</id>
		<lat>$entry.lat</lat>
		<lon>$entry.lon</lon>
		<alt>$entry.alt</alt>
		<passengers>$entry.parties</passengers>
		<battLevel>$entry.battLevel</battLevel>
		<travelPath>$entry.dest</travelPath>
		<lastUpdate>$entry.ts</lastUpdate>
	</Vehicle>
</VehicleInfo>
