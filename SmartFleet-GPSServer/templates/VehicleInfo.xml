$def with (vehicles)
<?xml version="1.0"?>
<VehicleInfo>
	$for entry in vehicles:
	<Vehicle>
		<id>$entry.id</id>
		<lat>$entry.lat</lat>
		<lon>$entry.lon</lon>
		<passengers>TODO</passengers>
		<battLevel>$entry.battLevel</battLevel>
		<travelPath>TODO</travelPath>
		<lastInteraction>TODO</lastInteraction>
		<lastUpdate>TODO</lastUpdate>
	</Vehicle>
</VehicleInfo>
